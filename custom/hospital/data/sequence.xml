<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="sequence_hospital_patient" model="ir.sequence">
            <field name="name">Hasta Sırası</field>
            <field name="code">hospital.patient.sequence</field>
            <field name="prefix">Sıra No</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>

        </record>
        <record id="sequence_hospital_appointment" model="ir.sequence">
            <field name="name">Hastane Randevu</field>
            <field name="code">hospital.appointment.sequence</field>
            <field name="prefix">Hastane Randevusu</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_doctor" model="ir.sequence">
            <field name="name">Doktor Sırası</field>
            <field name="code">doctors.sequence</field>
            <field name="prefix">Doktor Sıra Numarası</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>